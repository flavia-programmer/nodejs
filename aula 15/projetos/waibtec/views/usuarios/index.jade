extends ../layout

block content
  h1 Página de Usuários
  br
  a(href="/" title="Home") Voltar
  br
  br
  a(href="/usuarios/create" title="Cadastrar") Cadastrar
  br
  br
  table(class="table well")
    thead
      tr
        th ID:
        th Nome:
        th Login:
        th Senha:
        th Data:
    tbody
      each valor, i in lista
        tr
          td #{valor._id}
          td #{valor.nome}
          td #{valor.login}
          td #{valor.senha}
          td #{valor.data_cad}
      
  
